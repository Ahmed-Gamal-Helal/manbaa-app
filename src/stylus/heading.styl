/*header*/

.nav-bar-styling
    background-color: $black
    border: none
    margin-bottom: 0
    border-radius: 0
    border-top: thin solid#262322
    .container
        .navbar-header
            .navbar-toggle
                margin-top 27px
            .supmark
                .navbar-brand
                    color: $white
                    font-weight: bold
                    font-size: 30px
                    margin-top: 18px
                    letter-spacing: -1px
                    span 
                        color: $red
        #myNavbar
            ul.navbar-right
                font-weight: bold
                flex: 1
                font-size: 16px
                padding: 10px
                font-weight: bold
                font-family: Helvetica
                letter-spacing: -1px
                padding: 20px 10px 20px 10px
                li a  
                    color: $white
                    span 
                        color: $red
